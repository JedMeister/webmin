hba_conf=/etc/postgresql/*/main/pg_hba.conf
psql=/usr/bin/psql
start_cmd=systemctl start postgresql
stop_cmd=systemctl stop postgresql
basedb=template1
perpage=25
plib=
pass=
login=postgres
pid_file=/run/postgresql/*.pid
nodbi=0
dump_cmd=/usr/bin/pg_dump
rstr_cmd=/usr/bin/pg_restore
sameunix=1
access=*: *
blob_mode=0
add_mode=1
date_subs=0
style=0
max_dbs=50
simple_sched=0
access_own=0
max_text=1000
webmin_subs=0
